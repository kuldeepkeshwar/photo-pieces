{
  "name": "photo-pieces",
  "version": "1.14.0",
  "description": "photo-pieces game built in React",
  "keywords": [
    "photo-pieces",
    "react",
    "game",
    "web-game",
    "pwa"
  ],
  "main": "src/index.js",
  "dependencies": {
    "@babel/core": "7.0.0-beta.46",
    "@babel/runtime": "7.0.0-beta.46",
    "autoprefixer": "8.5.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "babel-loader": "8.0.0-beta.4",
    "babel-plugin-named-asset-import": "1.0.0-next.a671462c",
    "babel-preset-react-app": "4.0.0-next.a671462c",
    "bfj": "5.2.0",
    "body-parser": "^1.18.3",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "css-loader": "0.28.11",
    "dotenv": "5.0.1",
    "dotenv-expand": "4.2.0",
    "eslint": "4.19.1",
    "eslint-config-react-app": "3.0.0-next.a671462c",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.3",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.8.2",
    "express": "^4.16.3",
    "file-loader": "1.1.11",
    "fs-extra": "5.0.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.4.3",
    "loader-utils": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "morgan": "^1.9.0",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-loader": "2.1.5",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "16.4.2",
    "react-dev-utils": "6.0.0-next.a671462c",
    "react-dnd": "^5.0.0",
    "react-dnd-touch-backend": "^0.5.1",
    "react-dom": "16.4.2",
    "react-router-dom": "4.3.1",
    "resolve": "1.6.0",
    "sass-loader": "7.0.1",
    "serverless-http": "^1.6.0",
    "style-loader": "0.21.0",
    "svgr": "1.9.2",
    "sw-precache-webpack-plugin": "0.11.5",
    "thread-loader": "1.1.5",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "4.8.3",
    "webpack-dev-server": "3.1.5",
    "webpack-manifest-plugin": "2.0.3",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "express": "^4.16.3",
    "git-branch-is": "^2.0.0",
    "cross-env": "^5.2.0",
    "husky": "^1.0.0-rc.14",
    "netlify-lambda": "^0.4.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "preload-webpack-plugin": "^3.0.0-beta.2",
    "source-map-explorer": "^1.6.0",
    "sw-precache": "^5.2.1",
    "webpack-bundle-analyzer": "^3.0.2"
  },
  "scripts": {
    "postmerge": "git-branch-is master && npm version minor",
    "start": "npm-run-all --parallel start:app start:lambda",
    "start:app": "HTTPS=true node scripts/start.js",
    "start:lambda": "cross-env BABEL_ENV=development NODE_ENV=development  node netlify-lambda/bin/cmd.js serve src/lambda -c config/webpack.config.lamda.js",
    "build": "npm-run-all --parallel build:**",
    "build:app": "node scripts/build.js && npm run sw",
    "build:lambda": "BABEL_ENV=production NODE_ENV=production node netlify-lambda/bin/cmd.js build src/lambda -c config/webpack.config.lamda.js",
    "sw": "sw-precache --config=sw-precache-config.js",
    "test": "node scripts/test.js --env=jsdom",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.{js,jsx,mjs}",
      "**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.(graphql)$": "<rootDir>/config/jest/graphqlTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
